@{
    Layout = null;

    
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>@ViewBag.ProjectTitle</h1>
        <p>Type: @ViewBag.Type</p>
        <div>@Html.Raw(ViewBag.Description)</div>
        @if (ViewBag.Pictures.Length > 0)
        {
            <div>
                <h2>Pictures:</h2>
                <ul>
                    @foreach (var picture in ViewBag.Pictures)
                    {
                        <li>
                            <p>@picture</p>
                        </li>
                    }
                </ul>
            </div>
        }

    </div>
</body>
</html>
